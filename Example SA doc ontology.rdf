    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/2011/5/Ontology1308582601209.owl#Versioning_tools_ -->

    <owl:NamedIndividual rdf:about="&Ontology1308582601209;Versioning_tools_">
